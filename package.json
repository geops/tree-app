{
  "name": "@geops/tree-app",
  "description": "Web app for tree recommendations.",
  "license": "MIT",
  "version": "2.3.0",
  "private": true,
  "dependencies": {
    "@geops/tree-lib": "^2.0.0",
    "history": "4.10.1",
    "i18next": "20.6.1",
    "i18next-browser-languagedetector": "6.1.2",
    "lodash.difference": "4.5.0",
    "lodash.get": "4.4.2",
    "lodash.intersection": "4.4.0",
    "lodash.union": "4.6.0",
    "lodash.xor": "4.5.0",
    "mapbox-gl": "1.13.0",
    "ol": "6.7.0",
    "proj4": "2.7.5",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "11.12.0",
    "react-redux": "7.2.5",
    "react-router-dom": "5.3.0",
    "react-scripts": "4.0.3",
    "react-toastify": "8.0.2",
    "redux": "4.1.1",
    "redux-query-sync": "0.1.10",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "2.0.3",
    "workbox-background-sync": "^6.3.0",
    "workbox-broadcast-update": "^6.3.0",
    "workbox-cacheable-response": "^6.3.0",
    "workbox-core": "^6.3.0",
    "workbox-precaching": "^6.3.0",
    "workbox-routing": "^6.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "babel-eslint": "10.1.0",
    "csv-parse": "4.16.3",
    "cypress": "8.4.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-flowtype": "^6.0.1",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "lodash.set": "4.3.2",
    "prettier": "2.4.0",
    "react-test-renderer": "17.0.2",
    "start-server-and-test": "1.14.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "jest": true
    },
    "extends": [
      "airbnb",
      "plugin:cypress/recommended",
      "react-app",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    }
  },
  "eslintIgnore": [
    "*.json",
    "*.css"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-checkout": "if [[ $HUSKY_GIT_PARAMS =~ 1$ ]]; then yarn install --frozen-lockfile; fi",
      "post-merge": "yarn install --frozen-lockfile",
      "post-rebase": "yarn install"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "eslint src --fix",
      "prettier --write 'src/**/*.js'"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:ci": "start-server-and-test start http://localhost:3000 'cypress run --record'",
    "start": "react-scripts start",
    "prebuild": "cd lib && yarn build",
    "build": "react-scripts build",
    "lint": "eslint src",
    "format": "prettier --write 'src/**/*.js'",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!ol)/\"",
    "eject": "react-scripts eject",
    "i18n:convert2csv": "node ./src/i18n/convert2csv.js",
    "i18n:convert2json": "node ./src/i18n/convert2json.js"
  },
  "workspaces": [
    "lib"
  ]
}
